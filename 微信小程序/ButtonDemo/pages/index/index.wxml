<view class="container">
    <button size="mini">mini</button>
    <button>default</button>
    <button type="primary">primary</button>
    <button type="warn">warn</button>
    <button disabled="{{disabled}}" bindtap="changeDisabled">点击设置以上按钮disabled属性</button>
    <button plain="{{isPlain}}" bindtap="changePlain">点击设置以上按钮plain属性</button>
    <button loading="{{loading}}" bindtap="changeLoading">点击设置以上按钮loading属性</button>
    <button>default</button>
</view>
